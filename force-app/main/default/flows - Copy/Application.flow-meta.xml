    <formulas>
        <name>Current_Date</name>
        <dataType>Date</dataType>
        <expression>TODAY()</expression>
    </formulas>
    <formulas>
        <name>New_Date</name>
        <dataType>Date</dataType>
        <expression>CASE(
MOD({!New_Sufficiency_Review.MUSW__Due_Date__c} - DATE(1900, 1, 7), 7),
0, ({!New_Sufficiency_Review.MUSW__Due_Date__c}) + {!Counter}+ FLOOR(({!Counter}-1)/5)*2,
1, ({!New_Sufficiency_Review.MUSW__Due_Date__c}) + {!Counter}+ FLOOR(({!Counter})/5)*2,
2, ({!New_Sufficiency_Review.MUSW__Due_Date__c}) + {!Counter}+ FLOOR(({!Counter}+1)/5)*2,
3, ({!New_Sufficiency_Review.MUSW__Due_Date__c}) + {!Counter}+ FLOOR(({!Counter}+2)/5)*2,
4, ({!New_Sufficiency_Review.MUSW__Due_Date__c}) + {!Counter}+ FLOOR(({!Counter}+3)/5)*2,
5, ({!New_Sufficiency_Review.MUSW__Due_Date__c}) + {!Counter}+ CEILING(({!Counter})/5)*2,
6, ({!New_Sufficiency_Review.MUSW__Due_Date__c}) - IF({!Counter}&gt;0,1,0) + {!Counter}+ CEILING(({!Counter})/5)*2,
null)</expression>
    </formulas>
    <formulas>
        <name>Review_Due_Date</name>
        <dataType>Date</dataType>
        <expression>Today () + 3</expression>
    </formulas>
    <formulas>
        <name>Review_Due_Date_Formula</name>
        <dataType>Date</dataType>
        <expression>CASE(
MOD(TODAY() - DATE(1900, 1, 7), 7),
0, (TODAY()) + {!Get_Review_Template.MUSW__Due_in_Days__c}+ FLOOR(({!Get_Review_Template.MUSW__Due_in_Days__c}-1)/5)*2,
1, (TODAY()) + {!Get_Review_Template.MUSW__Due_in_Days__c}+ FLOOR(({!Get_Review_Template.MUSW__Due_in_Days__c})/5)*2,
2, (TODAY()) + {!Get_Review_Template.MUSW__Due_in_Days__c}+ FLOOR(({!Get_Review_Template.MUSW__Due_in_Days__c}+1)/5)*2,
3, (TODAY()) + {!Get_Review_Template.MUSW__Due_in_Days__c}+ FLOOR(({!Get_Review_Template.MUSW__Due_in_Days__c}+2)/5)*2,
4, (TODAY()) + {!Get_Review_Template.MUSW__Due_in_Days__c}+ FLOOR(({!Get_Review_Template.MUSW__Due_in_Days__c}+3)/5)*2,
5, (TODAY()) + {!Get_Review_Template.MUSW__Due_in_Days__c}+ CEILING(({!Get_Review_Template.MUSW__Due_in_Days__c})/5)*2,
6, (TODAY()) - IF({!Get_Review_Template.MUSW__Due_in_Days__c}&gt;0,1,0) + {!Get_Review_Template.MUSW__Due_in_Days__c}+ CEILING(({!Get_Review_Template.MUSW__Due_in_Days__c})/5)*2,
null)</expression>
    </formulas>

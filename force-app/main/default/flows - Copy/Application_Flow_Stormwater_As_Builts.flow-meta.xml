    <decisions>
        <name>Is_Permit_in_Final_Approval_and_SCM_Bond_Exists</name>
        <label>Is Permit in Final Approval and SCM Bond Exists</label>
        <locationX>182</locationX>
        <locationY>1274</locationY>
        <defaultConnector>
            <targetReference>Cannot_Submit_Yet</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes_Final_Approval_Phase</name>
            <conditionLogic>(1 AND 2) OR (1 AND 3)</conditionLogic>
            <conditions>
                <leftValueReference>GetStormwaterPermitRecord.MUSW__Phase__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Final Approval</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Permit_Bond.MUSW__Permit__r.Id</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Are_you_recording_your_stormwater_on_your_Utility_As_Built</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>No</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>As_Builts_Application_Intake</targetReference>
            </connector>
            <label>Yes Final Approval Phase</label>
        </rules>
    </decisions>

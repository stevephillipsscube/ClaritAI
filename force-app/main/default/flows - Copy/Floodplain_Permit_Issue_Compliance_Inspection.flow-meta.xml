        <label>Expires in 30 days</label>
        <locationX>182</locationX>
        <locationY>252</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ExpirationDateAsDate</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>OneMonthFromNow</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_ID_for_Engineering_Reviews_queue</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <description>JRS - adjusting process</description>
    <environments>Default</environments>
    <formulas>
        <name>ExpirationDateAsDate</name>
        <dataType>Date</dataType>
        <expression>DATETIMEVALUE({!$Record.MUSW__Expiration_Date__c})</expression>
    </formulas>
    <formulas>
        <name>FiveMonthsAgo</name>
        <dataType>Date</dataType>
        <expression>DATE(
    YEAR(TODAY()) - IF(MONTH(TODAY()) &gt; 5, 0, 1),
    MOD(MONTH(TODAY()) - 5 + 12, 12),
    DAY(TODAY())
)</expression>
    </formulas>
    <formulas>
        <description>Calculating one month from today</description>
        <name>OneMonthFromNow</name>
        <dataType>Date</dataType>
        <expression>DATE(
    YEAR(TODAY()) + IF(MONTH(TODAY()) = 12, 1, 0),
    MOD(MONTH(TODAY()) + 1 + 12, 12),
    DAY(TODAY())
)</expression>
    </formulas>
    <formulas>
        <name>SixMonthsAgo</name>
        <dataType>Date</dataType>
        <expression>DATE(
    YEAR(TODAY()) - IF(MONTH(TODAY()) &gt; 6, 0, 1),
    MOD(MONTH(TODAY()) - 6 + 12, 12),
    DAY(TODAY())
)</expression>
    </formulas>
    <formulas>
        <name>TestDate</name>
        <dataType>Date</dataType>
        <expression>TODAY()</expression>
    </formulas>

        <dataType>Number</dataType>
        <expression>DATE(YEAR({!Event_End_Date}), MONTH({!Event_End_Date}), DAY({!Event_End_Date})) - DATE(YEAR({!Event_Start_Date}), MONTH({!Event_Start_Date}), DAY({!Event_Start_Date}))</expression>

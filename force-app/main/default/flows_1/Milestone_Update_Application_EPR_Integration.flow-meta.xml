<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>55.0</apiVersion>
    <description>This autolaunched flow updates the EPR Integration value to Bluebeam, e-PlanSoft or ProjectDox on an Application when a child Milestone with matching conditions for EPR Integration is created.</description>
    <environments>Default</environments>
    <interviewLabel>Milestone - Update Permit EPR Integration {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Milestone - Update Application EPR Integration</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>Update_Application_EPR_Integration</name>
        <label>Update Application EPR Integration</label>
        <locationX>176</locationX>
        <locationY>323</locationY>
        <filterLogic>1 AND (2 OR 3 OR 4)</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.MUSW__Application2__r.Id</elementReference>
            </value>
        </filters>
        <filters>
            <field>EPR_Integration__c</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>Bluebeam</stringValue>
            </value>
        </filters>
        <filters>
            <field>EPR_Integration__c</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>e-PlanSoft</stringValue>
            </value>
        </filters>
        <filters>
            <field>EPR_Integration__c</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>ProjectDox</stringValue>
            </value>
        </filters>
        <inputAssignments>
            <field>EPR_Integration__c</field>
            <value>
                <elementReference>$Record.EPR_Integration__c</elementReference>
            </value>
        </inputAssignments>
        <object>MUSW__Application2__c</object>
    </recordUpdates>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Update_Application_EPR_Integration</targetReference>
        </connector>
        <filterLogic>1 AND (2 OR 3 OR 4)</filterLogic>
        <filters>
            <field>MUSW__Application2__c</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue></stringValue>
            </value>
        </filters>
        <filters>
            <field>EPR_Integration__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>e-PlanSoft</stringValue>
            </value>
        </filters>
        <filters>
            <field>EPR_Integration__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>ProjectDox</stringValue>
            </value>
        </filters>
        <filters>
            <field>EPR_Integration__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Bluebeam</stringValue>
            </value>
        </filters>
        <object>MUSW__Milestone__c</object>
        <recordTriggerType>Create</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Active</status>
</Flow>

            <name>Appeal</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Application.Application_Purpose__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Appeal</stringValue>
                </rightValue>
            </conditions>

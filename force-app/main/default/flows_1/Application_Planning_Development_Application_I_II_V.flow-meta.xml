            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Application.Application_Purpose__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>New</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Application.Application_Purpose__c</leftValueReference>
                <operator>EqualTo</operator>
